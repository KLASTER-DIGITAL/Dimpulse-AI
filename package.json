{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "simple-git-hooks": {
    "pre-push": "npm run check",
    "post-commit": "npm run git:push"
  },
  "scripts": {
    "dev": "tsx server/index.ts",
    "client": "vite",
    "start:dev": "npm run dev",
    "build": "vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:all": "npm run build && npm run build:server",
    "start": "NODE_ENV=production node dist/index.js",
    "vercel-build": "npm run build",
    "check:supabase": "tsx scripts/check-supabase-schema.ts",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "git:push": "git-auto-push",
    "git:commit": "git add . && git commit -m",
    "git:sync": "npm run git:commit && npm run git:push",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky",
    "analyze": "vite build --mode analyze",
    "docker:build": "docker build -t dimpulse-ai .",
    "docker:run": "docker run -p 3002:3002 dimpulse-ai",
    "docker:compose": "docker-compose up -d",
    "security:check": "npm audit"
  },
